{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "redirects": [
        {
            "source": "/(.*)",
            "destination": "https://vercel.com/docs"
        },
        {
            "source": "/",
            "destination": "/react-app-home.js",
            "permanent": true
        }
    ],
    "public": true
}